<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ApplicationManagement</fullName>
    <active>false</active>
    <errorConditionFormula>IF(AND(ISPICKVAL(Type, &quot;Application Management&quot;),CS_SkipProfiles__c =&apos;N&apos;), OR(ISBLANK(URL_of_Room_being_accessed__c),
