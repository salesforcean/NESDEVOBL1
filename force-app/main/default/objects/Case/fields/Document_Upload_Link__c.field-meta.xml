<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Document_Upload_Link__c</fullName>
    <description>Displays a link you can provide to the requester to upload a photo ID if they open the case over the phone or didn&apos;t already upload one via the CCPA web form.</description>
    <externalId>false</externalId>
    <formula>IF(NOT(ISBLANK(CaseNumber)), &quot;https://pearson.tfaforms.net/324?tfa_1461=&quot; &amp; CaseNumber, &quot;&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Document Upload Link</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
