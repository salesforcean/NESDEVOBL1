<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>User_Profile_Link__c</fullName>
    <description>Link to (v2) user profile.</description>
    <externalId>false</externalId>
    <formula>IF(AND(ISPICKVAL(Platform__c, &quot;Connexus v2&quot;), User_ID__c &gt; 0), &quot;https://www.connexus.com/webuser/overview.aspx?idWebuser=&quot; &amp; TEXT(User_ID__c), &quot;&quot;)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <inlineHelpText>Only appears if the Platform is Connexus v2 and the Subject User ID is filled out.</inlineHelpText>
    <label>User Profile Link</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Text</type>
    <unique>false</unique>
</CustomField>
