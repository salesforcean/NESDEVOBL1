    <validationRules>
        <fullName>C_Require_Data_Type_with_Comp_Value</fullName>
        <active>true</active>
        <errorConditionFormula>AND( ISBLANK(TEXT(Comparison_Data_Type__c)), NOT(ISBLANK( Comparison_Value__c)))</errorConditionFormula>
        <errorMessage>Data type is required when there is a comparison value.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>C_Select_Either_Value_or_Field</fullName>
        <active>true</active>
        <description>To ensure that both value and field are not set at the same time.</description>
        <errorConditionFormula>AND( NOT( ISBLANK( Comparison_Field__c ) ) ,  NOT( ISBLANK( Comparison_Value__c ) )  )</errorConditionFormula>
        <errorMessage>A Comparison Field and Comparison Value cannot be set at the same time. Please choose one.</errorMessage>
    </validationRules>
