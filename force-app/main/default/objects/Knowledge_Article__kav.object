    <validationRules>
        <fullName>Internal_Article_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(CONTAINS(RecordType.Name, &quot;Internal&quot;), AND(CONTAINS(RecordType.Name, &quot;Internal&quot;),IsVisibleInCsp), AND(CONTAINS(RecordType.Name, &quot;External&quot;),NOT(IsVisibleInCsp)))</errorConditionFormula>
        <errorMessage>Please update the &apos;Visible to Customer&apos; checkbox to be consistent with the Article Type.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Publish_Article_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>IF(ISCHANGED(PublishStatus),IF(AND($User.Department = &quot;PCx&quot;,RecordType.Name = &quot;LCC_Internal&quot;), true, false), false)</errorConditionFormula>
        <errorMessage>PCx Users are not allowed to publish LCC Internal Articles</errorMessage>
    </validationRules>
