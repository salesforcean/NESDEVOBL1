<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>ECC_Process_Has_Parent_Component</fullName>
    <active>true</active>
    <errorConditionFormula>AND(ISPICKVAL(Criteria_Type__c , &quot;Process&quot;),  ISBLANK( Enrollment_Component__c ))</errorConditionFormula>
