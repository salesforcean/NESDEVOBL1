        </processMetadataValues>
        <name>formula_4_myRule_3_A1_6016702976</name>
        <dataType>String</dataType>
        <expression>IF(ISNEW(),&apos;New&apos;,&apos;Old&apos;)</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>OR(
AND(ISCHANGED( [hed__Program_Enrollment__c].HowStudentPlacementCompleted__c ),OR( [hed__Program_Enrollment__c].HowStudentPlacementCompleted__c = &quot;Modified Auto-Placement Process&quot;, [hed__Program_Enrollment__c].HowStudentPlacementCompleted__c = &quot;School Recommended Auto-Placement Process&quot;, [hed__Program_Enrollment__c].HowStudentPlacementCompleted__c = &quot;Standard Auto-Placement Process&quot;)),
(and ( ISCHANGED([hed__Program_Enrollment__c].Program_Grade_Level__c) ,
          not isBlank([hed__Program_Enrollment__c].Program_Grade_Level__c))),AND(ISCHANGED([hed__Program_Enrollment__c].IEP__c),OR([hed__Program_Enrollment__c].IEP__c  = &quot;&quot;,[hed__Program_Enrollment__c].IEP__c  = &quot;IEP&quot;)),(OR(ISCHANGED([hed__Program_Enrollment__c].RCStatus__c),ISCHANGED([hed__Program_Enrollment__c].UTStatus__c))))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>OR(
AND(ISCHANGED( {!myVariable_current.HowStudentPlacementCompleted__c} ),OR( {!myVariable_current.HowStudentPlacementCompleted__c} = &quot;Modified Auto-Placement Process&quot;, {!myVariable_current.HowStudentPlacementCompleted__c} = &quot;School Recommended Auto-Placement Process&quot;, {!myVariable_current.HowStudentPlacementCompleted__c} = &quot;Standard Auto-Placement Process&quot;)),
(and ( ISCHANGED({!myVariable_current.Program_Grade_Level__c}) ,
          not isBlank({!myVariable_current.Program_Grade_Level__c}))),AND(ISCHANGED({!myVariable_current.IEP__c}),OR({!myVariable_current.IEP__c}  = &quot;&quot;,{!myVariable_current.IEP__c}  = &quot;IEP&quot;)),(OR(ISCHANGED({!myVariable_current.RCStatus__c}),ISCHANGED({!myVariable_current.UTStatus__c}))))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>ISCHANGED([hed__Program_Enrollment__c].OrderingHoldFlag__c) || (ISCHANGED([hed__Program_Enrollment__c].Status__c) &amp;&amp; (ISPICKVAL([hed__Program_Enrollment__c].Status__c, &apos;Complete&apos;) || ISPICKVAL(PRIORVALUE([hed__Program_Enrollment__c].Status__c), &apos;Complete&apos;))) || (ISNEW() &amp;&amp; [hed__Program_Enrollment__c].PE_Record_Type_Text__c =&apos;Student&apos;)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>ISCHANGED({!myVariable_current.OrderingHoldFlag__c}) || (ISCHANGED({!myVariable_current.Status__c}) &amp;&amp; (ISPICKVAL({!myVariable_current.Status__c}, &apos;Complete&apos;) || ISPICKVAL(PRIORVALUE({!myVariable_current.Status__c}), &apos;Complete&apos;))) || (ISNEW() &amp;&amp; {!myVariable_current.PE_Record_Type_Text__c} =&apos;Student&apos;)</expression>
    </formulas>
