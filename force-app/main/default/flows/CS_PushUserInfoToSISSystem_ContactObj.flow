    <decisions>
        <name>Is_Email_Empty</name>
        <label>Is Email Empty ?</label>
        <locationX>723</locationX>
        <locationY>116</locationY>
        <defaultConnector>
            <targetReference>Assign_Contact_Fields</targetReference>
        </defaultConnector>
        <defaultConnectorLabel>No</defaultConnectorLabel>
        <rules>
            <name>Yes</name>
            <conditionLogic>or</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Email</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
