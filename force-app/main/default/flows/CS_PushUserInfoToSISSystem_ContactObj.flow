                <rightValue>
                    <stringValue>Learning Coach</stringValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Is_Email_Empty</targetReference>
            </connector>
            <label>UserInfoUpdate</label>
        </rules>
    </decisions>
