            </connector>
            <label>Yes</label>
        </rules>
    </decisions>
    <decisions>
        <description>Check Record Type and Request Type</description>
        <name>ReqType_Not_Equals_UserInfo_Update</name>
        <label>ReqType Not Equals UserInfo Update</label>
        <locationX>440</locationX>
        <locationY>515</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>RecordTypeNotUserInfoUpdate</name>
            <conditionLogic>1 AND 2 AND (3 OR 4)</conditionLogic>
            <conditions>
                <leftValueReference>$Record.Request_Type__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
